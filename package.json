{
  "name": "bun-template",
  "module": "src/index.ts",
  "description": "<INSERT DESCRIPTION>",
  "version": "1.0.21",
  "keywords": [
    "webgl",
    "texture",
    "spritesheet",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jacklehamster/bun-template"
  },
  "homepage": "<INSERT HOMEPAGE>",
  "author": "Jack Le Hamster",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "types/src/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "bun run index.ts",
    "test": "bun test",
    "build": "rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "list": "bun run samples/list-scripts.tsx",
    "example": "cd example && bun start && cd ..",
    "build:example": "cd example && bun run build && cd ..",
    "fileSample": "bun run samples/file.tsx && cat samples/data/test.json",
    "httpSample": "bun run samples/server.tsx",
    "make-repo-ssh": "./make-repo-ssh.sh",
    "commit-all": "./commit-all.sh",
    "np": "bun run build && bun run build:example && bun run make-repo-ssh && bun run commit-all && np"
  },
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "@types/jest": "^29.5.6",
    "@types/mocha": "^10.0.3",
    "@types/react": "^18.2.21",
    "bun": "^1.0.7",
    "bun-types": "latest",
    "jest": "^29.7.0",
    "png-to-ico": "^2.1.8",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
